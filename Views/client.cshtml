@{
  Layout="Shared/_Layout.cshtml";
}
<h2>Here are all the clients with this stylist:</h2>

@if (@Model["clients"].Count == 0)
{
  <p>No clients have been added yet!</p>
}
else
{
  <ol>
    @foreach (var client in @Model["clients"])
    {
      <li>@client.GetName()</li>
    }
  </ol>
}
<hr>
<h4><a href="/clients/new">Add New</a></h4>
<h4><a href="/">Return to Main Page</a></h4>
